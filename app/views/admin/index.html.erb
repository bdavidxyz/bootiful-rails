<header class="main-content__header" role="banner">
  <h1 class="main-content__page-title" id="page-title">
    Series
  </h1>
  <form class="search" role="search">
    <label class="search__label" for="search">
      <svg class="search__eyeglass-icon" role="img">
        <title>
          Search Series
        </title>
        <use xlink:href="#icon-eyeglass">
        </use>
      </svg>
    </label>
    <input class="search__input" id="search" type="search" name="search" placeholder="Search Series" value="">
    <a class="search__clear-link" href="/admin/series">
      <svg class="search__clear-icon" role="img">
        <title>Clear search</title>
        <use xlink:href="#icon-cancel">
        </use>
      </svg>
    </a>
  </form>
  <div>
    <a class="button" href="/admin/series/new">New series</a>
  </div>
</header>
<section class="main-content__body main-content__body--flush">
  <table aria-labelledby="page-title">
    <thead>
      <tr>
        <th class="cell-label
        cell-label--number
        cell-label--false" scope="col" role="columnheader" aria-sort="none">
          <a href="/admin/series?series%5Bdirection%5D=asc&amp;series%5Border%5D=id">
            Id
          </a> </th>
        <th class="cell-label
        cell-label--string
        cell-label--false" scope="col" role="columnheader" aria-sort="none">
          <a href="/admin/series?series%5Bdirection%5D=asc&amp;series%5Border%5D=name">
            Name
          </a> </th>
        <th scope="col">
        </th>
        <th scope="col">
        </th>
      </tr>
    </thead>
    <tbody>
      <tr class="js-table-row" tabindex="0" role="link" data-url="/admin/series/2011">
        <td class="cell-data cell-data--number">
          <a href="/admin/series/2011" tabindex="-1" class="action-show">
            2011
          </a>
        </td>
        <td class="cell-data cell-data--string">
          <a href="/admin/series/2011" tabindex="-1" class="action-show">
            An example
          </a>
        </td>
        <td>
          <a class="action-edit" href="/admin/series/2011/edit">Edit</a>
        </td>
        <td>
          <a class="text-color-red" data-confirm="Are you sure?" rel="nofollow" data-method="delete" href="/admin/series/2011">Destroy</a>
        </td>
      </tr>
    </tbody>
  </table>
</section>
